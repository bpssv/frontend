    jQuery(document).ready(function($){
	var timelines = $('.slick-timeline-dots'),
		eventsMinDistance = 100;

	(timelines.length > 0) && initTimeline(timelines);

	function initTimeline(timelines) {
		timelines.each(function() {
			var timeline = $(this),
				dotComponent = {};
			//cache timeline components 
			dotComponent['timelineWrapper'] = timeline.find('.dots-container');
			dotComponent['eventsWrapper'] = dotComponent['timelineWrapper'].children('.events');
			dotComponent['fillingLine'] = dotComponent['eventsWrapper'].children('.filling-line');
			dotComponent['timelineEvents'] = dotComponent['eventsWrapper'].find('a');
			dotComponent['timelineDates'] = dotComponent['timelineEvents'];
            if(window.innerWidth > 1000) {
                 eventsMinDistance = window.innerWidth/(dotComponent['timelineDates'].length+1);
                eventsMinDistance -= 5;
            } 
           

          
            for (i = 0; i < dotComponent['timelineDates'].length; i++) { 
		    dotComponent['timelineEvents'].eq(i).css('left', (i+1)*eventsMinDistance+'px');
		}
			//assign a width to the timeline
			var timelineTotWidth = setTimelineWidth(dotComponent, eventsMinDistance);


			dotComponent['eventsWrapper'].on('click', 'a', function(event){
				event.preventDefault();
				dotComponent['timelineEvents'].removeClass('selected');
				$(this).addClass('selected');
                document.getElementById(`time${ event.target.id}`).click();
				// updateOlderEvents($(this));
				updateFilling($(this), dotComponent['fillingLine'], timelineTotWidth);
				// updateVisibleContent($(this), dotComponent['eventsContent']);
			});

            $('.timeline-next').on('click', (event) => {
                console.log(document.getElementById(`${$('a.selected')[0].id}`));
                document.getElementById(`${(Number($('a.selected')[0].id) +1)%10}`).click();
                // console.log($('a.selected')[0].id);
            })
            $('.timeline-prev').on('click', (event) => {
                console.log(document.getElementById(`${$('a.selected')[0].id}`));
                let number = Number($('a.selected')[0].id) -1;
                if(number < 0) {
                   number = 9;
                } 
                    document.getElementById(`${number%10}`).click();
  
            })

		
		});
	}

	

	function updateFilling(selectedEvent, filling, totWidth) {
		//change .filling-line length according to the selected event
		var eventStyle = window.getComputedStyle(selectedEvent.get(0), null),
			eventLeft = eventStyle.getPropertyValue("left"),
			eventWidth = eventStyle.getPropertyValue("width");
		eventLeft = Number(eventLeft.replace('px', '')) + Number(eventWidth.replace('px', ''))/2;
		var scaleValue = eventLeft/totWidth;
		setTransformValue(filling.get(0), 'scaleX', scaleValue);
	}

	function setTimelineWidth(dotComponent, width) {
		
		let	totalWidth = (dotComponent['timelineDates'].length + 1)*width;
		dotComponent['eventsWrapper'].css('width', totalWidth+'px');
		updateFilling(dotComponent['timelineEvents'].eq(0), dotComponent['fillingLine'], totalWidth);
	
		return totalWidth;
	}

	
	function setTransformValue(element, property, value) {
		element.style["-webkit-transform"] = property+"("+value+")";
		element.style["-moz-transform"] = property+"("+value+")";
		element.style["-ms-transform"] = property+"("+value+")";
		element.style["-o-transform"] = property+"("+value+")";
		element.style["transform"] = property+"("+value+")";
	}


});
</script>


