        body {
            font-family: sofia-pro, sans-serif;
            font-style: normal;
            font-weight: 100;
        }
        
        @media (min-width: 1000px) {
            .body-wrap {
                /* margin: 1rem; */
                border-bottom: 15px solid #fff;
            }
        }
        
        .body-wrap {
            overflow-x: hidden;
            position: relative;
            margin: 1rem;
        }
        
        .bpsHomeVideoContainer {
            position: relative;
            width: 100%;
            /* height: 100%; */
            overflow: hidden;
        }
        
        .body-wrap .bpsHomeVideoContainer video {
            object-fit: cover;
            display: block;
            min-height: 100%;
            min-width: 100%;
            width: auto;
            max-height: none;
            max-width: none;
            backface-visibility: hidden;
            perspective: 1000;
        }
        
        .home-wrap-container {
            position: absolute;
            z-index: 1;
            width: 100%;
            /* height: 100%; */
        }
        
        .title-strip {
            -webkit-animation-name: mymove;
            animation-name: mymove
        }
        
        @keyframes mymove {
            0% {
                opacity: 0;
                filter: blur(10px);
                transform: scaleX(1.75);
            }
            100% {
                opacity: 1;
                filter: blur(0);
                transform: scaleX(1);
            }
        }
        
        button.explore {
            cursor: pointer;
            padding: 0.71429em 1.07143em;
            font-family: "Roboto Condensed", sans-serif;
            font-size: 0.875em;
            text-transform: uppercase;
            background: #363E47;
            border: 0;
            box-shadow: inset 0 0 0 0.14286em transparent;
            color: #fff;
            font-weight: 700;
            transition: background 0.3s, box-shadow 0.3s, color 0.3s;
        }
        
        button.explore span::after {
            position: absolute;
            left: 0;
            width: 100%;
            height: 100%;
            -moz-transform-origin: left;
            transform-origin: left;
        }
        
        button.explore:hover {
            background: var(--blue);
        }
        
        .loading span {
            width: 25px;
            height: 25px;
            margin: 0 5px;
            background-coLor: #000;
            border-radius: 50%;
            display: inline-block;
            animation-name: dots;
            animation-duration: 2s;
            animation-iteration-count: infinite;
            animation-timing-function: ease-in-out;
        }
        
        .loading :nth-child(2) {
            background-coLor: #000;
            animation-delay: 0.4s;
        }
        
        ._fold_1 div::before {
            content: '';
            width: 100%;
            height: 100%;
            position: absolute;
            left: 0;
            top: 0;
            /* z-index: 2; */
            background-color: rgba(0, 0, 0, 0.1);
            background-image: linear-gradient(to top, rgb(0 0 0 / 19%) 0%, rgb(0 0 0 / 15%) 10%, rgba(0, 0, 0, 0.05) 30%, rgba(0, 0, 0, 0.05) 70%, rgba(0, 0, 0, 0.5) 90%, rgba(0, 0, 0, 0.5) 100%);
        }
        /* Header start */
        
        .sticky {
            box-shadow: 0 3px 12px 0 rgba(0, 0, 0, .08);
            position: fixed !important;
            background: var(--blue) !important;
            top: 0;
            width: 100%;
            min-height: 7rem;
            z-index: 3;
            transform: translateY(-100%);
            animation: sticky-slide-down .4s ease .25s forwards;
        }
        
        .header-out {
            transition: all 0.4s;
        }
        
        @keyframes sticky-slide-down {
            100% {
                transform: translateY(0);
            }
        }
        
        .sticky .top-nav-bar-container {
            display: none;
        }
        
        .sticky .header-container {
            padding-top: 2rem;
        }
        /* Header end */
        
        .sidenav {
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 10;
            top: 0;
            left: 0;
            background-color: #1B2736;
            color: #fff;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 1rem;
        }
        
        .card {
            background-color: rgba(11, 54, 91, 0.71);
            border-top: 1px solid rgb(128, 128, 128);
            border-bottom: 1px solid grey;
        }
        /* .show {
            background-color: rgba(21, 107, 182, 0.71);
        } */
        
        .card-header {
